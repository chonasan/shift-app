<h3 class="bg-light mt-5 ml-5 px-5 w-25 text-center">ジャンル編集</h3>
<div class="container ml-5">
  <%= form_with model: @task, url: admin_task_path(@task.id), local:true do |f| %>
    <div class="row pt-5">
      <h6 class="col-2 pt-2 pl-2">作業名</h6>
      <%= f.text_field :work_name, class: "col-3 mr-5" %>
      <%= f.submit '変更を保存', class: 'btn btn-success col-2' %>
    </div>
  <% end %>
</div>