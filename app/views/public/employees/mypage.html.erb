<h1>Public::Employees#my_page</h1>
<p>Find me in app/views/public/employees/my_page.html.erb</p>

<div class="container">
  <div class='row'>
    <div class="col-lg-12">
      <h1 class="mt-2 mb-3">マイページ</h1>
    </div>
   </div>

  <div class='row mb-3'>
   <div class="col-lg-1">
     登録情報
   </div>
   <div class="col-lg-2">
     <%= link_to "編集する", employees_edit_path, class:"btn btn-success" %>
   </div>
  </div>
</div>

<div class="container">
  <div class='row'>
   <div class="col-lg-7">
    <table class="table table-bordered">
     <tbody>
        <tr>
            <th>氏名</th>
            <td><%= @employee.first_name%><%= @employee.last_name %></td>
        </tr>
        <tr>
            <th>カナ</th>
            <td><%= @employee.first_name_kana%><%= @employee.last_name_kana%></td>
        </tr>
         <tr>
            <th>郵便番号</th>
            <td><%= @employee.postal_code%></td>
        </tr>
         <tr>
            <th>住所</th>
            <td><%= @employee.address%></td>
        </tr>
         <tr>
            <th>電話番号</th>
            <td><%= @employee.telephone_number%></td>
        </tr>
         <tr>
            <th>メールアドレス</th>
            <td><%= @employee.email%></td>
        </tr>
     </tbody>
    </table>
　　</div>
　</div>
</div>